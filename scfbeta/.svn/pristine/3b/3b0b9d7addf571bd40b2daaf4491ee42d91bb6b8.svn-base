<!-- 机构借款统计表的模态框 -->
<form class="form-horizontal" role="form" id="orgnAddForm" data-toggle="validator">
	<input type="hidden" id="recUid" name="recUid">
	<!-- modal -->
	<div class="modal fade" id="orgnAddModal" aria-hidden="true"
		data-backdrop="static" tabindex="-1" role="dialog"
		aria-labelledby="addModalLabel">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal"
						aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
					<h4 class="modal-title" id="orgnAddModalLabel">添加</h4>
				</div>
				<div class="modal-body">
					<div class="form-group">
						<label class="col-sm-4 control-label" for="loanDate">贷款时间</label>
						<div class="col-sm-6">
							<span style="position: relative; z-index: 9999;"> <input
								id="loanDate" name="loanDate" class="form-control time"
								style="cursor: pointer;" readonly>
							</span> <span class="add-on"
								style="margin-top: -31px; margin-right: 10px;"><i
								class="glyphicon glyphicon-calendar"></i></span>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-4 control-label" for="dueDate">到期日</label>
						<div class="col-sm-6">
							<span style="position: relative; z-index: 9999;"> <input
								id="dueDate" name="dueDate" class="form-control"
								style="cursor: pointer;" readonly>
							</span> <span class="add-on"
								style="margin-top: -31px; margin-right: 10px;"><i
								class="glyphicon glyphicon-calendar"></i></span>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-4 control-label" for="loanBank">借款银行</label>
						<div class="col-sm-6">
							<input class="form-control" name="loanBank" id="loanBank"
								required>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-4 control-label" for="assureCompany">担保公司</label>
						<div class="col-sm-6">
							<input class="form-control" name="assureCompany"
								id="assureCompany" required>
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-4 control-label" for="cautionMoney">保证金(担保公司)</label>
						<div class="col-sm-6">
							<input class="form-control" name="cautionMoney" id="cautionMoney">
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-4 control-label" for="loanMoney">金额(元)</label>
						<div class="col-sm-6">
							<input class="form-control" name="loanMoney" id="loanMoney">
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-4 control-label" for="profitRate">利率(%)</label>
						<div class="col-sm-6">
							<input class="form-control" name="profitRate" id="profitRate">
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-4 control-label" for="pawn">抵押物</label>
						<div class="col-sm-6">
							<input class="form-control" name="pawn" id="pawn" required>
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<div class="form-group">
						<div class="col-sm-4"></div>
						<div class="col-sm-3">
							<button type="button" class="btn btn-default cancel"
								data-dismiss="modal">取消</button>
						</div>
						<div class="col-sm-3">
							<button type="submit" id="btn_save" class="btn btn-default add">保存</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</form>

<!-- 对外担保明细模态框 -->
<form class="form-horizontal" role="form" id="guaranteeAddForm" data-toggle="validator">
	<input type="hidden" id="recUid" name="recUid">
	<div class="modal fade" id="guaranteeAddModal" tabindex="-1" role="dialog"
		aria-labelledby="addModalLabel">
		<div class="modal-dialog" role="document">
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal"
						aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
					<h4 class="modal-title" id="guaranteeAddModalLabel">添加</h4>
				</div>
				<div class="modal-body">
					<div class="form-group">
						<label class="col-sm-4 control-label" for="guarantorName"><span
							class="required">*</span>被担保方名称</label>
						<div class="col-sm-6">
							<input type="text" name="guarantorName" class="form-control"
								id="guarantorName" placeholder="被担保方名称">
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-4 control-label" for="guaranteeMoney"><span
							class="required">*</span>担保金额</label>
						<div class="col-sm-6">
							<input type="text" name="guaranteeMoney" class="form-control"
								id="guaranteeMoney">
						</div>
					</div>
					<div class="form-group">
						<label class="col-sm-4 control-label" for="guaranteeType"><span
							class="required">*</span>担保类型</label>
						<div class="col-sm-6">
							<select id="guaranteeType" name="guaranteeType" class="form-control">
								<option value="0">保证</option>
								<option value="1">抵押</option>
								<option value="2">质押</option>
								<option value="3">留置</option>
								<option value="4">定金</option>
							</select>
						</div>
					</div>
				</div>
				<div class="modal-footer">
					<div class="form-group">
						<div class="col-sm-4"></div>
						<div class="col-sm-3">
							<button type="button" class="btn btn-default cancel"
								data-dismiss="modal">取消</button>
						</div>
						<div class="col-sm-3">
							<button type="submit" id="btn_save" class="btn btn-default add">保存</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</form>



