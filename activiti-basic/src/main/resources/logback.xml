<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <Pattern>%date{HH:mm:ss.SSS} %-5level %logger:%line - %msg%n</Pattern>
        </encoder>
    </appender>

    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>

	<logger name="org.apache.ibatis.datasource.pooled.PooledDataSource" level="info"/>

</configuration>
