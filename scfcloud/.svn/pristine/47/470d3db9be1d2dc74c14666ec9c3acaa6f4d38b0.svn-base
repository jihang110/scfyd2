						<div class="form-group">
							<label class="col-sm-4 control-label">特定放款条件</label> <label
								id="spcLendCondition" name="spcLendCondition"
								class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">综合保理成本</label> <label
								id="factCmprCost" name="factCmprCost"
								class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">回款方式</label> <label
								id="paymentMethod" name="paymentMethod"
								class="col-sm-6 control-label left"></label>
						</div>
						<fieldset id="fsAdvice">
							<legend class="little-title">审批意见</legend>
							<div class="form-group">
								<label class="col-sm-4 control-label">意见</label>
								<div class="col-sm-6">
									<textarea rows="6" name="advice" class="form-control"
										id="advice" readonly></textarea>
								</div>
							</div>
						</fieldset>
					</form>
				</div>
				<div class="modal-footer">
					<div class="form-group">
						<div class="col-sm-2 col-sm-offset-10">
							<button type="button" class="btn btn-default"
								data-dismiss="modal">&nbsp;取消&nbsp;</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
    <!-- 项目尽调详情弹出模态框 -->
	<div class="modal fade" style="z-index: 1051" id="projectTuningModal"
		tabindex="-1" role="dialog"
		aria-labelledby="projectTuningModalLabel">
