					</table>
				</div>
				<div class="modal-footer">
					<div class="form-group">
						<div class="col-sm-2 col-sm-offset-10">
							<button type="button" class="btn btn-default"
								data-dismiss="modal">&nbsp;取消&nbsp;</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- 立项审批 -->
	<div class="modal fade" style="z-index: 1051" id="projectSetupModal"
		tabindex="-1" role="dialog" aria-labelledby="projectSetupModalLabel">
