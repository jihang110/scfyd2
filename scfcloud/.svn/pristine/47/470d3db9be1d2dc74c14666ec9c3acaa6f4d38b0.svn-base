						<div class="form-group">
							<label class="col-sm-4 control-label">供应商名称</label> <label
								id="custName" name="custName"
								class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">核心企业名称</label> <label
								id="coreCorpName" name="coreCorpName"
								class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">合同编号</label> <label
								id="cntrctNo" name="cntrctNo"
								class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">组织机构代码</label> <label
								id="orgNo" name="orgNo" class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">币种</label> <label
								id="ccy" name="ccy" class="col-sm-6 control-label left"></label>
						</div>

						<div class="form-group">
							<label class="col-sm-4 control-label">合同反馈序号</label> <label
								id="cntrctSerialNo" name="cntrctSerialNo"
								class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">起始日</label> <label
								id="valDt" name="valDt" class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">合同金额</label> <label
								id="creditLimit" name="creditLimit"
								class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">应收账款总额</label> <label
								id="arAmt" name="arAmt" class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">到期日</label> <label
								id="dueDt" name="dueDt" class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">放款金额</label> <label
								id="loanAmt" name="loanAmt"
								class="col-sm-6 control-label left"></label>
						</div>
						<div class="form-group">
							<label class="col-sm-4 control-label">融资比例</label> <label
								id="loanRt" name="loanRt" class="col-sm-6 control-label left"></label>
						</div>
						<fieldset id="fsAdvice">
							<legend class="little-title">审批意见</legend>
							<div class="form-group">
								<label class="col-sm-4 control-label">意见</label>
								<div class="col-sm-6">
									<textarea rows="6" name="advice" class="form-control"
										id="advice" readonly></textarea>
								</div>
							</div>
						</fieldset>
					</form>
					
					<div class="row" style="margin-bottom: 10px;">
						<div class="col-sm-12" style="line-height: 40px;">
							<strong class="text-muted">应收账款信息</strong>
						</div>
					</div>
					<table id="receiveAccountManagerList" class="table table-bordered" cellspacing="0"
